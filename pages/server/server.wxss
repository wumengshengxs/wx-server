/**server.wxss**/

.section{
  height: 30px;
  width: 90%;
}
.section input{
  width:90%;
  margin:5px auto;
  border:1px solid #c3c3c3;
  height:30px;
  border-radius: 3px;
  padding: 0 5px;
}
.map_container{
  position: absolute;
  top: 42px;
  bottom: 0px;
  left: 0;
  right: 0;
}
.map{
  width: 100%;
  height: 100%;
}
.map_text{
  position: absolute;
  left: 0;
  right: 0;
  bottom: 0px;
  height: 80px;
  background: #fff;
  padding: 0 15px;
}

.h1{
  margin: 15px 0;
  font-size:15px;
}
.text_box{
  margin: 10px 25px;
  border-bottom:1px solid #c3c3c3;
  padding-bottom:10px
}

.place_image{
  height: 100rpx;
  width: 300rpx;
  position: absolute;
  left: 30%;
  top: 90%;
}

.jie_image{
  height: 300rpx;
  width: 300rpx;
  position: absolute;
  left: 30%;
  top: 76%;
}

.place_left_img{
    height:100rpx;
    width:100rpx;
    position: absolute;
    left: 6%;
    top: 90%;
}
.place_top_img{
    height:100rpx;
    width:100rpx;
    position: absolute;
    left: 90%;
    top: 890rpx;
}

.place_rigin_img{
    height:100rpx;
    width:100rpx;
    position: absolute;
    left: 80%;
    top: 90%;
}

.place_image-top{
    height:66rpx;
    width:70rpx;
    position: absolute;
    left: 90%;
    top:10rpx;
}

.site{
  height:320rpx;
  width:200rpx;
  left: 1%;
  background-color: white; 
  position: absolute;
  top: 300rpx;
  border-radius: 8px;
}
.cancel_image{
  height: 36rpx;
  width: 36rpx;
}

.cancel{
    display: flex;
    justify-content: flex-end;
    
}
.cancel-text{
  font-size: 30rpx;
  color: #7e7e7e;
  white-space:pre-line;
  margin-top: -18rpx;
}

.sercer-wid{
  width: 100%;
}
